//a program to add five numbers
#include<stdio.h>

int main()
{
    //declaration and initialization of variables
    int num1 = 5;
    int num2 = 6;
    int num3 = 8;
    int num4 = 3;
    int num5 = 1;
    
    int sum; 
    sum = num1 + num2 + num3 + num4 + num5;
    
    //output the sum 
    printf("\tYour sum = %d",sum);
   
     return 0;
    
}